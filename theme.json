{
    "$schema": "https://schemas.wp.org/trunk/theme.json",
    "version": 2,
    "settings": {
        "appearanceTools": false,
        "color": {
            "background": true,
            "custom": false,
            "customDuotone":false,
            "customGradient": false,
            "defaultDuotone": false,
            "defaultGradients": false,
            "defaultPalette": true,
            "duotone": [],
            "gradients": [],
            "link": false,
            "palette": [
                {
                    "slug": "black",
                    "color": "#000000",
                    "name": "Black"
                },
                {
                    "slug": "white",
                    "color": "#FFFFFF",
                    "name": "White"
                },
                {
                    "slug": "gray",
                    "color": "#868686",
                    "name": "Gray"
                },
                {
                    "slug": "red",
                    "color": "#FA5B3D",
                    "name": "Red"
                },
                {
                    "slug": "green",
                    "color": "#86D7BB",
                    "name": "Green Light"
                },
                {
                    "slug": "blue",
                    "color": "#1621F7",
                    "name": "Blue"
                },
                {
                    "slug": "orange",
                    "color": "#EE8054",
                    "name": "Orange"
                },
                {
                    "slug": "yellow",
                    "color": "#FEC550",
                    "name": "Jaune"
                },
                {
                    "slug": "rose",
                    "color": "#F6BCCB",
                    "name": "Rose"
                },
                {
                    "slug": "turquoise",
                    "color": "#86CDD7",
                    "name": "Turquoise"
                }
            ],
            "text": true
        },
        "layout": {
            "contentSize": "835px",
            "wideSize": "1260px"
        },
        "spacing": {
            "blockGap": null,
            "margin": false,
            "padding": false,
            "units": [ "%", "px", "vh", "vw" ]
        },
        "typography": {
            "customFontSize": false,
            "fontStyle": false,
            "fontWeight": false,
            "letterSpacing": false,
            "lineHeight": false,
            "textDecoration": true,
            "textTransform": false,
            "dropCap": false,
            "fontSizes": [
                {
                    "slug": "small",
                    "size": "11px",
                    "name": "Small"
                },
                {
                    "slug": "normal",
                    "size": "14px",
                    "name": "Normal"
                },
                {
                    "slug": "large",
                    "size": "16px",
                    "name": "Large"
                },
                {
                    "slug": "xlarge",
                    "size": "16px",
                    "name": "Extra Large"
                },
                {
                    "slug": "big",
                    "size": "20px",
                    "name": "Big"
                },
                {
                    "slug": "h1",
                    "size": "46px",
                    "name": "H1"
                },
                {
                    "slug": "h2",
                    "size": "46px",
                    "name": "H2"
                },
                {
                    "slug": "h3",
                    "size": "28px",
                    "name": "H3"
                },
                {
                    "slug": "h4",
                    "size": "22px",
                    "name": "H4"
                },
                {
                    "slug": "h5",
                    "size": "20px",
                    "name": "H5"
                },
                {
                    "slug": "h6",
                    "size": "18px",
                    "name": "H6"
                }
            ],
            "fontFamilies": [
                {
                    "fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell, \"Helvetica Neue\",sans-serif",
                    "slug": "system-font",
                    "name": "System Font"
                },
                {
                    "fontFamily": "Helvetica Neue, Helvetica, Arial, sans-serif",
                    "slug": "helvetica-arial",
                    "name": "Helvetica or Arial"
                },
                {
                    "fontFamily": "Poppins, Helvetica Neue, Helvetica, Arial, sans-serif",
                    "slug": "secondary",
                    "name": "Poppins"
                },
                {
                    "fontFamily": "Montserrat, Helvetica Neue, Helvetica, Arial, sans-serif",
                    "slug": "primary",
                    "name": "Montserrat"
                }
            ]
        },
        "blocks": {
            "core/group": {
                "spacing": {
                    "padding": true,
                    "blockGap": true
                }
            },
            "core/heading": {
                "typography": {
                    "textTransform": true
                }
            },
            "core/buttons": {
                "spacing": {
                    "margin": true,
                    "blockGap": true
                }
            },
            "core/button": {
                "border": {
                    "radius": false
                },
                "typography": {
                    "textTransform": true,
                    "fontFamilies": []
                }
            }
        },
        "custom": {
            "menu": {
                "fontFamily": "var(--wp--preset--font-family--secondary)",
                "fontSize": "15px",
                "fontWeight": 500
            }
        }
    },
    "styles": {
        "color": {
            "background": "var(--wp--preset--color--white)",
            "text": "var(--wp--preset--color--black)"
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--secondary)",
            "fontSize": "var(--wp--preset--font-size--normal)",
            "fontWeight": "normal"
        },
        "elements": {
            "link": {
                "color": {
                    "text": "inherit"
                }
            },
            "h1": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-1)"
                }
            },
            "h2": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-2)"
                }
            },
            "h3": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-3)"
                }
            },
            "h4": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-4)"
                }
            },
            "h5": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-5)"
                }
            },
            "h6": {
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--h-6)"
                }
            }            
        },
        "blocks": {
            "core/paragraph": {
                "spacing": {
                    "margin": {
                        "top": "0",
                        "bottom": "var(--goldencat--spacing-vertical)"
                    }
                }
            },
            "core/heading": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)",
                    "fontWeight": "normal"
                },
                "spacing": {
                    "margin": {
                        "top": "0",
                        "bottom": "var(--goldencat--spacing-vertical)"
                    }
                }
            },
            "core/button": {
                "typography": {
                    "fontFamily": "var(--wp--preset--font-family--secondary)",
                    "fontWeight": "bold",
                    "fontSize": "12px",
                    "letterSpacing": "2px",
                    "textDecoration": "none",
                    "textTransform": "uppercase",
                    "lineHeight": "1"
                },
                "color": {
                    "background": "var(--goldencat--color-primary)",
                    "text": "var(--goldencat--color-background)"
                },
                "border": {
                    "color": "var(--goldencat--color-background)",
                    "radius": "0",
                    "style": "solid",
                    "width": "1px"
                },
                "spacing": {
                    "padding": "var(--goldencat--button--padding-vertical) var(--goldencat--button--padding-horizontal)"
                }
            }
        }
    }
}
